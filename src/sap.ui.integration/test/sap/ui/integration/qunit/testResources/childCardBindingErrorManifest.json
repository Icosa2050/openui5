{
	"_version": "1.73.1",
	"sap.app": {
		"id": "card.explorer.sample.childCardUsingDestinations.childCard",
		"type": "card",
		"title": "Sample of destinations",
		"subTitle": "Sample of destinations",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"shortTitle": "A short title for this card",
		"info": "Additional information about this card",
		"description": "A long description for this card",
		"tags": {
			"keywords": [
				"Destinations",
				"Card",
				"Sample"
			]
		}
	},
	"sap.card": {
		"type": "Object",
		"data": {
			"json": {
				"value": [
					{
						"Logo": "ui5",
						"ProductID": 1,
						"ProductName": "Chai",
						"SupplierID": 1,
						"CategoryID": 1,
						"QuantityPerUnit": "10 boxes x 20 bags",
						"UnitPrice": "18.0000",
						"UnitsInStock": 39,
						"UnitsOnOrder": 0,
						"ReorderLevel": 10,
						"Discontinued": false,
						"Image": "Woman_avatar_01"
					}
				]
			}
		},
		"configuration": {
			"parameters": {
				"ProductID": {
					"value": 1
				}
			}
		},
		"header": {
			"data": {
				"path": "/value/0"
			},
			"title": "{ProductName}",
			"icon": {
				"src": "./images/{Image}.png"
			}
		},
		"content": {
			"data": {
				"path": "/value/0"
			},
			"groups": [
				{
					"items": [
						{
							"label": "ID",
							"value": "{ProductID}"
						},
						{
							"label": "Units in stock",
							"value": "{UnitsInStock}"
						},
						{
							"label": "Unit Price",
							"value": "{= format.currency(${UnitPrice}, 'EUR')}"
						}
					]
				}
			]
		}
	}
}