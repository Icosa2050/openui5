<mvc:View
	controllerName="sap.f.cardsdemo.controller.Destinations"
	displayBlock="true"
	xmlns:widgets="sap.ui.integration.widgets"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
>
	<Page title="Destinations" class="sapUiContentPadding">
		<VBox>
			<Title level="H2" text="Card" class="sapUiLargeMarginTop sapUiSmallMarginBottom" />
			<widgets:Card id="card1" manifest="{cardsPlayground>/playgroundBaseUrl}bundles/cardWithDestinations/manifest.json" />
			<Title level="H2" text="Card with Child with &quot;sap.card/configuration/useMainDestinations&quot;" class="sapUiLargeMarginTop sapUiSmallMarginBottom" />
			<widgets:Card id="card2" manifest="{cardsPlayground>/playgroundBaseUrl}bundles/cardWithDestinationsAndChild/manifest.json" />
			<Title level="H2" text="Card with Child with &quot;sap.card/configuration/useMainDestinations&quot; and Own Destinations" class="sapUiLargeMarginTop sapUiSmallMarginBottom" />
			<widgets:Card id="card3" manifest="{cardsPlayground>/playgroundBaseUrl}bundles/cardWithDestinationsChildOwnDestinations/manifest.json" />
			<Title level="H2" text="Card with Child with Own Destinations Only (Deprecated)" class="sapUiLargeMarginTop sapUiSmallMarginBottom" />
			<widgets:Card id="card4" manifest="{cardsPlayground>/playgroundBaseUrl}bundles/cardDestinationsChildOwnDestinationsOnly/manifest.json" />
		</VBox>
	</Page>
</mvc:View>